<div class="page-header">
  <h1><%= @event.name %></h1>
  <h2><%= @session.name %>
    <small><%= @session.timeslot %></small>
  </h2>
  <% if @table.persisted? %>
    <h3><%= @table.long_name %></h3>
  <% end %>

</div>
<h4>Add Table to <%= @session.name %></h4>

<%= render 'form' %>

<%= link_to 'Back', event_session_tables_path, :class => 'btn btn-success' %>
